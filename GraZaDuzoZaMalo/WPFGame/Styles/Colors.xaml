<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ApplicationBackground">#9fc1d9</Color>
    <SolidColorBrush x:Key="ApplicationBackgroundBrush" Color="{StaticResource ApplicationBackground}" />

    <Color x:Key="BorderBackground">#1b3344</Color>
    <SolidColorBrush x:Key="BorderBackgroundBrush" Color="{StaticResource BorderBackground}" />

    <Color x:Key="ButtonBackground">#40779f</Color>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="{StaticResource ButtonBackground}" />

    <Color x:Key="TextBoxBackground">#DCE6ED</Color>
    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="{StaticResource TextBoxBackground}" />

</ResourceDictionary>